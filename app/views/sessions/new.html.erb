<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :user_id %><br />
    <%= text_field_tag :user_id, params[:user_id] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>